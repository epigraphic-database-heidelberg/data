            --><div type="edition" xml:lang="la"><head>Text</head><ab><lb n="1"/>
                <expan><abbr>Ter</abbr><ex>tius</ex></expan> Vale<lb break="no" n="2"/>rius <expan><abbr>C</abbr><ex>ai</ex></expan> <expan><abbr>f</abbr><ex>ilius</ex></expan> <lb n="3"/>Aniensis <lb n="4"/>Cremona <lb n="5"/><expan><abbr>mil</abbr><ex>es</ex></expan> <expan><abbr>leg</abbr><ex>ionis</ex></expan> XV <expan><abbr>Ap</abbr><ex>ollinaris</ex></expan> <lb n="6"/><expan><abbr><supplied reason="lost">an</supplied>nor</abbr><ex>um</ex></expan> XXV <lb n="0"/><gap reason="lost" extent="unknown" unit="line"/>
