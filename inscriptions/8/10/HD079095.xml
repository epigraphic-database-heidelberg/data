                Vo<supplied reason="lost">seg</supplied>o <expan><abbr>Sil</abbr><ex>vestri</ex></expan> <expan><abbr>s</abbr><ex>acrum</ex></expan> Adnam<lb break="no" n="2"/>us Nertomari <expan><abbr>fil</abbr><ex>ius</ex></expan> <lb n="3"/><expan><abbr>v</abbr><ex>otum</ex></expan> <expan><abbr>s</abbr><ex>olvit</ex></expan> <expan><abbr>l</abbr><ex>ibens</ex></expan> <lb n="4"/><expan><abbr>l</abbr><ex>aetus</ex></expan> <expan><abbr>m</abbr><ex>erito</ex></expan>
