            --><div type="edition" xml:lang="la"><head>Text</head><ab><lb n="1"/>
                <supplied reason="lost">Nero Claudius</supplied> <lb n="2"/><supplied reason="lost">Caesa</supplied>r Au<supplied reason="lost">gustus</supplied> <lb n="3"/><supplied reason="lost">Ger</supplied>mani<supplied reason="lost">cus</supplied> <lb n="4"/><supplied reason="lost">pont</supplied>ifex max<supplied reason="lost">imus</supplied> <lb n="5"/><supplied reason="lost">pontif</supplied>icis maxi<supplied reason="lost">mi <expan><abbr>f</abbr><ex>ilius</ex></expan></supplied> <lb n="0"/><gap reason="lost" extent="unknown" unit="line"/><certainty match="preceding-sibling::node()" locus="value"/>
