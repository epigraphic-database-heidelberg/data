                <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <expan><abbr>Fl</abbr><ex>avius</ex></expan> <lb n="2"/>Felix <expan><abbr>sig</abbr><ex>nifer</ex></expan> d<supplied reason="lost">e</supplied> <lb n="3"/><expan><abbr>n</abbr><ex>umero</ex></expan> <expan><abbr>Divit</abbr><ex>iensium</ex></expan> vixit <lb n="4"/><expan><abbr>an</abbr><ex>nos</ex></expan> XXX civis Am<lb break="no" n="5"/>bianensis <gap reason="lost" extent="unknown" unit="character"/>
