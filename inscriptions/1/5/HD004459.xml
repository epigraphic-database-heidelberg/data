            --><div type="edition" xml:lang="la"><head>Text</head><ab><lb n="1"/>
                <expan><abbr>I</abbr><ex>ovi</ex></expan> <expan><abbr>O</abbr><ex>ptimo</ex></expan> <expan><abbr>M</abbr><ex>aximo</ex></expan> et Iu<lb break="no" n="2"/><expan><abbr>no</abbr><ex>ni</ex></expan> <lb n="3"/><expan><abbr>T</abbr><ex>itus</ex></expan> <expan><abbr>Carm</abbr><ex cert="low">inius</ex></expan> Scra<lb break="no" n="4"/><expan><abbr>ini</abbr><ex cert="low">us</ex></expan> <lb n="5"/><expan><abbr>momor</abbr><ex>iae</ex></expan> <note>sic</note> Quinti <lb n="6"/>Quinti <lb n="7"/><expan><abbr>d</abbr><ex>ono</ex></expan> <expan><abbr>d</abbr><ex>edit</ex></expan>
