                <expan><abbr>Libert</abbr><ex>is</ex></expan> <expan><abbr>libertab</abbr><ex>usque</ex></expan> <lb n="2"/><expan><abbr>M</abbr><ex>arci</ex></expan> Clodi Galli <lb n="3"/>his <expan><abbr>q</abbr><ex>ui</ex></expan> <expan><abbr>i</abbr><ex>nfra</ex></expan> <expan><abbr>s</abbr><ex>cripti</ex></expan> <expan><abbr>s</abbr><ex>unt</ex></expan> <lb n="4"/>Clodiae <expan><abbr>M</abbr><ex>arci</ex></expan> <expan><abbr>l</abbr><ex>ibertae</ex></expan> Primae <lb n="5"/><expan><abbr>M</abbr><ex>arco</ex></expan> Clodio Nestori <lb n="6"/>Clodiae <expan><abbr>M</abbr><ex>arci</ex></expan> <expan><abbr>l</abbr><ex>ibertae</ex></expan> Cinurae <lb n="7"/><expan><abbr>M</abbr><ex>arco</ex></expan> Clodio Cataphyllo <lb n="8"/>Clodiae <expan><abbr>M</abbr><ex>arci</ex></expan> <expan><abbr>l</abbr><ex>ibertae</ex></expan> Chiae <lb n="9"/><expan><abbr>M</abbr><ex>arco</ex></expan> Clodio Sabino <lb n="10"/><expan><abbr>M</abbr><ex>arco</ex></expan> Clodio Celeri <lb n="11"/><expan><abbr>M</abbr><ex>arco</ex></expan> Clodio Iucundo <lb n="12"/>in <expan><abbr>fr</abbr><ex>onte</ex></expan> <expan><abbr>p</abbr><ex>edes</ex></expan> XV <lb n="13"/>in <expan><abbr>agr</abbr><ex>o</ex></expan> <expan><abbr>p</abbr><ex>edes</ex></expan> XVI
